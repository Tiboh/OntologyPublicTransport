    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#customDate -->

    <rdfs:Datatype rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#customDate"/>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#customTime -->

    <rdfs:Datatype rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#customTime"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#from -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#from">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_day"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Transfer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Stop"/>
        <rdfs:comment xml:lang="en">Identifies a stop where a connection between routes begins.</rdfs:comment>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_day -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_day">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_stop"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Service"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Day"/>
        <rdfs:comment xml:lang="en">A service is available for specified days.</rdfs:comment>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_direction -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_direction">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Trip"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Direction"/>
        <rdfs:comment xml:lang="en">Indicates the direction of travel for a trip.</rdfs:comment>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_service -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_service">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Trip"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Service"/>
        <rdfs:comment xml:lang="en">Identifies a set of dates when service is available for one or more routes.</rdfs:comment>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_stop -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_stop">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#StopTime"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Stop"/>
        <rdfs:comment xml:lang="en">Uniquely identifies a stop. Multiple routes may use the same stop.</rdfs:comment>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_transfer_type -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_transfer_type">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Transfer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#TransferType"/>
        <rdfs:comment xml:lang="en">Specifies the type of connection for the specified transfer.</rdfs:comment>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_transport_type -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_transport_type">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Trip"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#TransportType"/>
        <rdfs:comment xml:lang="en">Describes the type of transportation used on a route.</rdfs:comment>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_trip -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#has_trip">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#StopTime"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Trip"/>
        <rdfs:comment xml:lang="en">Identifies a trip.</rdfs:comment>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#part_of_route -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#part_of_route">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Trip"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Route"/>
        <rdfs:comment xml:lang="en">Identifies a route.</rdfs:comment>
    </ObjectProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#to -->

    <ObjectProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#to">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#from"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Transfer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Stop"/>
        <rdfs:comment xml:lang="en">Identifies a stop or station where a connection between routes ends.</rdfs:comment>
    </ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#arrival_time -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#arrival_time">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#StopTime"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#customTime"/>
        <rdfs:comment xml:lang="en">The arrival_time specifies the arrival time at a specific stop for a specific trip on a route. The time is measured from &quot;noon minus 12h&quot; (effectively midnight, except for days on which daylight savings time changes occur) at the beginning of the service date. For times occurring after midnight on the service date, enter the time as a value greater than 24:00:00 in HH:MM:SS local time for the day on which the trip schedule begins. If you don&apos;t have separate times for arrival and departure at a stop, enter the same value for arrival_time and departure_time.

If this stop isn&apos;t a time point, use an empty string value for the arrival_time and departure_time fields. Stops without arrival times will be scheduled based on the nearest preceding timed stop. To ensure accurate routing, please provide arrival and departure times for all stops that are time points. Do not interpolate stops.

You must specify arrival and departure times for the first and last stops in a trip.

Times must be eight digits in HH:MM:SS format (H:MM:SS is also accepted, if the hour begins with 0). Do not pad times with spaces.</rdfs:comment>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#color -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#color">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Route"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">In systems that have colors assigned to routes, the route_color field defines a color that corresponds to a route. The color must be provided as a six-character hexadecimal number, for example, 00FFFF. If no color is specified, the default route color is white (FFFFFF).</rdfs:comment>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#departure_time -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#departure_time">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#StopTime"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#customTime"/>
        <rdfs:comment xml:lang="en">The departure_time specifies the departure time from a specific stop for a specific trip on a route. The time is measured from &quot;noon minus 12h&quot; (effectively midnight, except for days on which daylight savings time changes occur) at the beginning of the service date. For times occurring after midnight on the service date, enter the time as a value greater than 24:00:00 in HH:MM:SS local time for the day on which the trip schedule begins. If you don&apos;t have separate times for arrival and departure at a stop, enter the same value for arrival_time and departure_time.

If this stop isn&apos;t a time point, use an empty string value for the arrival_time and departure_time fields. Stops without arrival times will be scheduled based on the nearest preceding timed stop. To ensure accurate routing, please provide arrival and departure times for all stops that are time points. Do not interpolate stops.

You must specify arrival and departure times for the first and last stops in a trip.

Times must be eight digits in HH:MM:SS format (H:MM:SS is also accepted, if the hour begins with 0). Do not pad times with spaces.</rdfs:comment>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#description -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#description">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Route"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Stop"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Contains a description of the instance. Please provide useful, quality information.</rdfs:comment>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#end_date -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#end_date">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#start_date"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Service"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#customDate"/>
        <rdfs:comment xml:lang="en">The end_date field contains the end date for the service. This date is included in the service interval.

The end_date field&apos;s value should be in YYYYMMDD format.</rdfs:comment>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#id -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#id">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Route"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Service"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Stop"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Trip"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#unsignedLong"/>
        <rdfs:comment xml:lang="en">Contains an ID that uniquely identifies the instance.</rdfs:comment>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#latitude -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#latitude">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Stop"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">Contains the latitude of a stop or station. The field value must be a valid WGS 84 latitude.</rdfs:comment>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#long_name -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#long_name">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#name"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Route"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Contains the full name of a route. This name is generally more descriptive than the short_name.</rdfs:comment>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#longitude -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#longitude">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Stop"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
        <rdfs:comment xml:lang="en">Contains the longitude of a stop or station. The field value must be a valid WGS 84 longitude value from -180 to 180.</rdfs:comment>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#min_transfer_time -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#min_transfer_time">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Transfer"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
        <rdfs:comment xml:lang="en">When a connection between routes requires an amount of time between arrival and departure (TransferType=TimedTransfer), the min_transfer_time field defines the amount of time that must be available in an itinerary to permit a transfer between routes at these stops. The min_transfer_time must be sufficient to permit a typical rider to move between the two stops, including buffer time to allow for schedule variance on each route.

The min_transfer_time value must be entered in seconds.</rdfs:comment>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#name -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#name">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Stop"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Please use a name that people will understand in the local and tourist vernacular.</rdfs:comment>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#short_name -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#short_name">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#name"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Route"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Trip"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#start_date -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#start_date">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#id"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Service"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#customDate"/>
        <rdfs:comment xml:lang="en">The start_date field contains the start date for the service.

The start_date field&apos;s value should be in YYYYMMDD format.</rdfs:comment>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#stop_sequence -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#stop_sequence">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#StopTime"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <rdfs:comment xml:lang="en">The stop_sequence field identifies the order of the stops for a particular trip. The values for stop_sequence must be non-negative integers, and they must increase along the trip.

For example, the first stop on the trip could have a stop_sequence of 1, the second stop on the trip could have a stop_sequence of 23, the third stop could have a stop_sequence of 40, and so on.</rdfs:comment>
    </DatatypeProperty>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#text_color -->

    <DatatypeProperty rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#text_color">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#color"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Route"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Can be used to specify a legible color to use for text drawn against a background of route color. The color must be provided as a six-character hexadecimal number, for example, FFD700.</rdfs:comment>
    </DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#BusType -->

    <Class rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#BusType">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#TransportType"/>
    </Class>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#CableCarType -->

    <Class rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#CableCarType">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#TransportType"/>
        <rdfs:comment xml:lang="en">Cable car. Used for street-level cable cars where the cable runs beneath the car.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Day -->

    <Class rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Day">
        <rdfs:comment xml:lang="en">Specifies days of a week.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Direction -->

    <Class rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Direction">
        <rdfs:comment xml:lang="en">Indicates the direction of travel for a trip.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#FerryType -->

    <Class rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#FerryType">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#TransportType"/>
        <rdfs:comment xml:lang="en">Ferry. Used for short- and long-distance boat service.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Friday -->

    <Class rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Friday">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#Day"/>
    </Class>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#FunicularType -->

    <Class rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#FunicularType">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#TransportType"/>
        <rdfs:comment xml:lang="en">Funicular. Any rail system designed for steep inclines.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#GondolaType -->

    <Class rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#GondolaType">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#TransportType"/>
        <rdfs:comment xml:lang="en">Gondola, Suspended cable car. Typically used for aerial cable cars where the car is suspended from the cable.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#ImmediateTransfer -->

    <Class rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#ImmediateTransfer">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#TransferType"/>
    </Class>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#ImpossibleTransfer -->

    <Class rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#ImpossibleTransfer">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#TransferType"/>
        <rdfs:comment xml:lang="en">Transfers are not possible between routes at this location.</rdfs:comment>
    </Class>
    


    <!-- http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#MetroType -->

    <Class rdf:about="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#MetroType">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thibault/ontologies/2016/0/public-transport#TransportType"/>
